{
  "tasks": {
    "dev": "deno run -A mod.ts",
    "release": "deno run -A --unstable https://deno.land/x/ndeno@v1.2.0/release.ts",
    "install": "deno install --allow-read --allow-write --allow-run -rfn nzip mod.ts"
  },
  "fmt": {
    "options": {
      "semiColons": false
    }
  },
  "compilerOptions": {
   "types": ["npm:@types/node"]
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  }
}
